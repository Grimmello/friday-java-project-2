<!--category.vtl-->
#if ($team)
  <h1>$team.getTeamName() members:</h1>
  <ul>
    #foreach($member in $team.getTeamMembers())
    <li><a href="/members/$member.getMemberID()">$member.getMemberName()</a></li>
   #end
  </ul>
#else
  <em><p>Oops! We can't seem to find that team.</p></em>
#end

<a href="/teams/$team.getID()/members/new">Add a new member</a>
<a href="/teams">All teams</a>
<!--team.vtl-->
