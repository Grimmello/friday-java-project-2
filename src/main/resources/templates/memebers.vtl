<!--members.vtl-->
<h1>Member List</h1>

#if ($members)
  <ul>
    #foreach ($member in $members)
      <li><a href="/members/$member.getId()">$member.getMemberName()</a></li>
    #end
  </ul>
#else
  <p>No members!</p>
#end

<a href="teams/$team.getTeamID()/members/new">Add New Member</a>
